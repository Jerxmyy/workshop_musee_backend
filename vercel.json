{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "FRONTEND_URL": "https://workshop-musee.vercel.app",
    "CORS_ORIGINS": "https://workshop-musee.vercel.app,http://localhost:5173,http://localhost:3000"
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}
